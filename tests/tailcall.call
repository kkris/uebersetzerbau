extern long neg(long);
extern long abs2(long);
extern long countup(long);
extern long countdown(long);

RAISES(neg, 1);
CHECK(neg, 0, 0);
CHECK(neg, 1, -1);
CHECK(neg, 2, -2);
CHECK(neg, -1, 1);
CHECK(neg, -2, 2);
CHECK(neg, 42, -42);
CHECK(neg, -1337, 1337);

RAISES(abs2, 1);
CHECK(abs2, 0, 0);
CHECK(abs2, 1, 1);
CHECK(abs2, 2, 2);
CHECK(abs2, -1, 1);
CHECK(abs2, -2, 2);
CHECK(abs2, 42, 42);
CHECK(abs2, -1337, 1337);

RAISES(countup, 1);
CHECK(countup, 0, 25);
CHECK(countup, 1, 25);
CHECK(countup, -42, 25);
CHECK(countup, 24, 25);

RAISES(countdown, 1);
CHECK(countdown, 0, 0);
CHECK(countdown, 1, 0);
CHECK(countdown, 24, 0);


