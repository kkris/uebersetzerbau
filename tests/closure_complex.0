gt = fun x ->
    let
        y = (x + x) - x // x
    in
        fun z ->
            y < z
        end
    end
end;

times2 = fun x -> x * 2 end;

map = fun func -> fun list ->
    if islist list then
        (func (head list)) . (map func (tail list))
    else
        func list
    end
end end;

sum = fun list -> (sumrec 0) list end;

sumrec = fun acc -> fun list ->
    if islist list then
        (sumrec (acc + (head list))) (tail list)
    else
        acc + list
    end
end end;

range = fun n -> (rangerec (n - 1)) n end;

rangerec = fun n -> fun list ->
    if n = 0 then
        0.list
    else
        (rangerec (n - 1)) (n.list)
    end
end end;

g1 = fun x -> (gt 0) x end;
g2 = fun x -> (gt 100) x end;
